{
  "Node": {
    "Port": 4949,

    "Modules": [
      "Munin.Node.Plugins.Hardware.dll",
      "Munin.Node.Plugins.PerformanceCounter.dll"
    ]
  },

  "Hardware": {
    "Sensor": [
      // Basic
      {
        "Name": "cpu_usage",
        "Hardware": [ "Cpu" ],
        "Sensor": "Load",
        "GraphCategory": "basic",
        "GraphTitle": "CPU usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "gpu_usage",
        "Hardware": [ "GpuNvidia" ],
        "Sensor": "Load",
        "Include": [
          { "Name": "GPU Core" },
          { "Name": "GPU Memory Controller" }
        ],
        "GraphCategory": "basic",
        "GraphTitle": "GPU usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "memory_usage",
        "Hardware": [ "Memory" ],
        "Sensor": "Load",
        "GraphCategory": "basic",
        "GraphTitle": "Memory usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "network_usage",
        "Hardware": [ "Network" ],
        "Sensor": "Load",
        "GraphCategory": "basic",
        "GraphTitle": "Network usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "network_traffic",
        "Hardware": [ "Network" ],
        "Sensor": "Throughput",
        "GraphCategory": "basic",
        "GraphTitle": "Network traffic",
        "GraphVLabel": "B/s",
        "GraphArgs": "-b 1024 -l 0"
      },
      {
        "Name": "storage_throughput",
        "Hardware": [ "Storage" ],
        "Sensor": "Throughput",
        "GraphCategory": "basic",
        "GraphTitle": "Storage throughput",
        "GraphVLabel": "B/s",
        "GraphArgs": "-b 1024 -l 0"
      },
      // Smart
      {
        "Name": "smart_data",
        "Hardware": [ "Storage" ],
        "Sensor": "Data",
        "GraphCategory": "smart",
        "GraphTitle": "SMART data read/write",
        "GraphVLabel": "GB",
        "GraphArgs": "-b 1024 -l 0"
      },
      {
        "Name": "smart_life",
        "Hardware": [ "Storage" ],
        "Sensor": "Level",
        "Include": [ { "Name": "Percentage Used" } ],
        "GraphCategory": "smart",
        "GraphTitle": "SMART life",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "smart_spare",
        "Hardware": [ "Storage" ],
        "Sensor": "Level",
        "Include": [ { "Name": "Available Spare" } ],
        "GraphCategory": "smart",
        "GraphTitle": "SMART spare",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      // Temperature
      {
        "Name": "cpu_temperature",
        "Hardware": [ "Cpu" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "CPU temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "motherboard_temperature",
        "Hardware": [ "SuperIO" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "Motherboard temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "gpu_temperature",
        "Hardware": [ "GpuNvidia" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "GPU temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "storage_temperature",
        "Hardware": [ "Storage" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "Storage temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Clock
      {
        "Name": "cpu_clock",
        "Hardware": [ "Cpu" ],
        "Sensor": "Clock",
        "Exclude": [ { "Name": "Bus Speed" } ],
        "GraphCategory": "hardware",
        "GraphTitle": "CPU clock",
        "GraphVLabel": "GHz",
        "GraphArgs": "-b 1000 -l 0",
        "Multiply": 0.001
      },
      {
        "Name": "gpu_clock",
        "Hardware": [ "GpuNvidia" ],
        "Sensor": "Clock",
        "GraphCategory": "hardware",
        "GraphTitle": "GPU clock",
        "GraphVLabel": "GHz",
        "GraphArgs": "-b 1000 -l 0",
        "Multiply": 0.001
      },
      // Fan
      {
        "Name": "fan",
        "Hardware": [ "SuperIO", "GpuNvidia" ],
        "Sensor": "Fan",
        "GraphCategory": "hardware",
        "GraphTitle": "FAN",
        "GraphVLabel": "rpm",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Power
      {
        "Name": "power",
        "Hardware": [ "Cpu" ],
        "Sensor": "Power",
        "Include": [ { "Name": "Package" } ],
        "GraphCategory": "hardware",
        "GraphTitle": "Power",
        "GraphVLabel": "W",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Voltage
      {
        "Name": "voltage",
        "Hardware": [ "Cpu" ],
        "Sensor": "Voltage",
        "Include": [
          { "Name": "Core (SVI2 TFN)" },
          { "Name": "SoC (SVI2 TFN)" }
        ],
        "GraphCategory": "hardware",
        "GraphTitle": "Voltage",
        "GraphVLabel": "V",
        "GraphArgs": "-b 1000 -l 0"
      }
    ],
    "Memory": {
      "Name": "memory",
      "Enable": true,
      "GraphTitle": "Memory",
      "GraphCategory": "basic"
    }
  },

  "PerformanceCounter": {
    "Counter": [
