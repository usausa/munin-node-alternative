{
  "Node": {
    "Port": 4949,

    "Modules": [
      "Munin.Node.Plugins.Hardware.dll",
      "Munin.Node.Plugins.PerformanceCounter.dll"
    ]
  },

  "Hardware": {
    "Sensor": [
      // CPU
      {
        "Name": "cpu_load",
        "Hardware": [ "Cpu" ],
        "Sensor": "Load",
        "GraphCategory": "cpu",
        "GraphTitle": "CPU usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "cpu_clock",
        "Hardware": [ "Cpu" ],
        "Sensor": "Clock",
        "Exclude": [ { "Name": "Bus Speed" } ],
        "GraphCategory": "cpu",
        "GraphTitle": "CPU clock",
        "GraphVLabel": "MHz",
        "GraphArgs": "-b 1000 -l 0"
      },
      // GPU
      {
        "Name": "gpu_load",
        "Hardware": [ "GpuNvidia" ],
        "Sensor": "Load",
        "Include": [
          { "Name": "GPU Core" },
          { "Name": "GPU Memory Controller" }
        ],
        "GraphCategory": "gpu",
        "GraphTitle": "Gpu usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "gpu_clock",
        "Hardware": [ "GpuNvidia" ],
        "Sensor": "Clock",
        "GraphCategory": "gpu",
        "GraphTitle": "GPU clock",
        "GraphVLabel": "MHz",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Memory
      {
        "Name": "memory_load",
        "Hardware": [ "Memory" ],
        "Sensor": "Load",
        "GraphCategory": "memory",
        "GraphTitle": "Memory usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      // Storage
      {
        "Name": "storage_used",
        "Hardware": [ "Storage" ],
        "Sensor": "Load",
        "Include": [ { "Name": "Used Space" } ],
        "GraphCategory": "storage",
        "GraphTitle": "Storage usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "storage_data",
        "Hardware": [ "Storage" ],
        "Sensor": "Data",
        "GraphCategory": "storage",
        "GraphTitle": "Storage data",
        "GraphVLabel": "GB",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "storage_life",
        "Hardware": [ "Storage" ],
        "Sensor": "Level",
        "Include": [ { "Name": "Percentage Used" } ],
        "GraphCategory": "storage",
        "GraphTitle": "Storage life",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "storage_spare",
        "Hardware": [ "Storage" ],
        "Sensor": "Level",
        "Include": [ { "Name": "Available Spare" } ],
        "GraphCategory": "storage",
        "GraphTitle": "Storage spare",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      // Network
      {
        "Name": "network_load",
        "Hardware": [ "Network" ],
        "Sensor": "Load",
        "GraphCategory": "network",
        "GraphTitle": "Network usage",
        "GraphVLabel": "%",
        "GraphArgs": "-l 0 -u 100",
        "GraphScale": false
      },
      {
        "Name": "network_data",
        "Hardware": [ "Network" ],
        "Sensor": "Data",
        "GraphCategory": "network",
        "GraphTitle": "Network data",
        "GraphVLabel": "GB",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Temperature
      {
        "Name": "cpu_temperature",
        "Hardware": [ "Cpu" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "CPU temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "motherboard_temperature",
        "Hardware": [ "SuperIO" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "Motherboard temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "gpu_temperature",
        "Hardware": [ "GpuNvidia" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "GPU temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "storage_temperature",
        "Hardware": [ "Storage" ],
        "Sensor": "Temperature",
        "GraphCategory": "temperature",
        "GraphTitle": "Storage temperature",
        "GraphVLabel": "C",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Fan
      {
        "Name": "fan",
        "Hardware": [ "SuperIO", "GpuNvidia" ],
        "Sensor": "Fan",
        "GraphCategory": "fan",
        "GraphTitle": "FAN",
        "GraphVLabel": "rpm",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Power
      {
        "Name": "power",
        "Hardware": [ "Cpu" ],
        "Sensor": "Power",
        "Include": [ { "Name": "Package" } ],
        "GraphCategory": "fan",
        "GraphTitle": "Power",
        "GraphVLabel": "W",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Voltage
      {
        "Name": "voltage",
        "Hardware": [ "Cpu" ],
        "Sensor": "Voltage",
        "Include": [
          { "Name": "Core (SVI2 TFN)" },
          { "Name": "SoC (SVI2 TFN)" }
        ],
        "GraphCategory": "fan",
        "GraphTitle": "Voltage",
        "GraphVLabel": "V",
        "GraphArgs": "-b 1000 -l 0"
      }
    ],
    "Memory": {
      "Name": "memory",
      "Enable": true
    }
  },

  "PerformanceCounter": {
    "Counter": [
      // CPU
      //{
      //  "Name": "cpu_usage",
      //  "Object": [
      //    {
      //      "Category": "Processor",
      //      "Counter": "% Processor Time"
      //    }
      //  ],
      //  "GraphCategory": "cpu",
      //  "GraphTitle": "CPU usage",
      //  "GraphVLabel": "%",
      //  "GraphArgs": "-l 0 -u 100",
      //  "GraphScale": false
      //},
      {
        "Name": "cpu_queue",
        "Object": [
          {
            "Category": "System",
            "Counter": "Processor Queue Length"
          }
        ],
        "GraphCategory": "cpu",
        "GraphTitle": "Processor queue length",
        "GraphVLabel": "queue length",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Memory
      {
        "Name": "memory_page",
        "Object": [
          {
            "Category": "Memory",
            "Counter": "Pages/sec"
          }
        ],
        "GraphCategory": "memory",
        "GraphTitle": "Memory pages/sec",
        "GraphVLabel": "pages/sec",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Storage
      //{
      //  "Name": "storage_free",
      //  "Object": [
      //    {
      //      "Category": "LogicalDisk",
      //      "Counter": "% Free Space"
      //    }
      //  ],
      //  "GraphCategory": "storage",
      //  "GraphTitle": "Disk free",
      //  "GraphVLabel": "%",
      //  "GraphArgs": "-l 0 -u 100",
      //  "GraphScale": false
      //},
      {
        "Name": "storage_time",
        "Object": [
          {
            "Category": "LogicalDisk",
            "Counter": "% Disk Time"
          }
        ],
        "GraphCategory": "storage",
        "GraphTitle": "Disk time",
        "GraphVLabel": "%",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "storage_queue",
        "Object": [
          {
            "Category": "PhysicalDisk",
            "Counter": "Current Disk Queue Length"
          }
        ],
        "GraphCategory": "storage",
        "GraphTitle": "Disk queue length",
        "GraphVLabel": "queue length",
        "GraphArgs": "-b 1000 -l 0"
      },
      // Network
      {
        "Name": "network_connections",
        "Object": [
          {
            "Category": "TCPv4",
            "Counter": "Connections Established"
          },
          {
            "Category": "TCPv6",
            "Counter": "Connections Established"
          }
        ],
        "GraphCategory": "network",
        "GraphTitle": "Network connections",
        "GraphVLabel": "number of connections",
        "GraphArgs": "-b 1000 -l 0"
      },
      // System
      {
        "Name": "process",
        "Object": [
          {
            "Category": "System",
            "Counter": "Processes"
          },
          {
            "Category": "Process",
            "Counter": "Thread Count",
            "Instance": "_Total"
          }
        ],
        "GraphCategory": "system",
        "GraphTitle": "Process",
        "GraphVLabel": "number of process",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "handle",
        "Object": [
          {
            "Category": "Process",
            "Counter": "Handle Count",
            "Instance": "_Total"
          }
        ],
        "GraphCategory": "system",
        "GraphTitle": "Handle",
        "GraphVLabel": "number of handle",
        "GraphArgs": "-b 1000 -l 0"
      },
      {
        "Name": "uptime",
        "Object": [
          {
            "Category": "System",
            "Counter": "System Up Time",
            "Multiply": 1.1574074074074073e-005 // (1 / 86400.0)
          }
        ],
        "GraphCategory": "system",
        "GraphTitle": "Uptime",
        "GraphVLabel": "days",
        "GraphArgs": "-b 1000 -l 0",
        "GraphDraw": "AREA"
      },
      // TODO IIS
      // TODO MSSQL
      // Hyper-V
      {
        "Name": "vm_count",
        "Object": [
          {
            "Category": "Hyper-V Virtual Machine Health Summary",
            "Counter": "Health Ok"
          },
          {
            "Category": "Hyper-V Virtual Machine Health Summary",
            "Counter": "Health Critical"
          },
          {
            "Category": "Hyper-V VM Vid Driver",
            "Counter": "VidPartitions"
          }
        ],
        "GraphCategory": "vm",
        "GraphTitle": "Virtual machine",
        "GraphVLabel": "number of vm",
        "GraphArgs": "-b 1000 -l 0"
      }
    ]
  },

  "Logging": {
    "EventLog": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    }
  }
}
