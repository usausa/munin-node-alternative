{
  "Node": {
    "Port": 14949,

    "Modules": [
      "Munin.Node.Plugins.Hardware",
      "Munin.Node.Plugins.PerformanceCounter"
    ]
  },

  "Hardware": {
    "MotherboardControl": { "Enable": true },
    "MotherboardFan": { "Enable": true },
    "MotherboardTemperature": { "Enable": true },
    "MotherboardVoltage": { "Enable": true },
    "CpuClock": { "Enable": true },
    "CpuLoad": { "Enable": true },
    "CpuPower": { "Enable": true },
    "CpuTemperature": { "Enable": true },
    "CpuVoltage": { "Enable": true },
    "MemoryLoad": { "Enable": true },
    "MemoryData": { "Enable": true },
    "GpuClock": { "Enable": true },
    "GpuControl": { "Enable": true },
    "GpuFan": { "Enable": true },
    "GpuLoad": { "Enable": true },
    "GpuPower": { "Enable": true },
    "GpuTemperature": { "Enable": true },
    "GpuVoltage": { "Enable": true },
    "StorageData": { "Enable": true },
    "StorageLevel": { "Enable": true },
    "StorageTemperature": { "Enable": true },
    "StorageThroughput": { "Enable": true },
    "StorageUsed": { "Enable": true },
    "NetworkData": { "Enable": true },
    "NetworkLoad": { "Enable": true },
    "NetworkThroughput": { "Enable": true }
  },

  "PerformanceCounter": {
    "Counters": [
      {
        "Name": "uptime",
        "Object": "System",
        "Counter": "System Up Time",
        "Multiply": 1.1574074074074073e-005,
        "GraphCategory": "system",
        "GraphTitle": "Uptime",
        "GraphArgs": "--base 1000 -l 0",
        "GraphDraw": "AREA"
      }
    ]
  },

  "Logging": {
    "EventLog": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    }
  }
}
